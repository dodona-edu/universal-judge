{
  "general": {
    "dependencies": [
      "values.js"
    ],
    "selector": false
  },
  "extensions": {
    "file": "js",
    "templates": ["js", "mako"]
  },
  "naming_conventions": {
    "namespace": "camel_case",
    "function": "camel_case",
    "identifier": "camel_case",
    "global_identifier": "macro_case",
    "property": "camel_case",
    "class": "pascal_case"
  },
  "constructs": {
    "objects": true,
    "exceptions": true,
    "function_calls": true,
    "assignments": true,
    "heterogeneous_collections": true,
    "heterogeneous_arguments": true,
    "evaluation": true,
    "default_parameters": true,
    "global_variables": true
  },
  "datatypes": {
    "integer": "supported",
    "real": "supported",
    "char": "reduced",
    "text": "supported",
    "boolean": "supported",
    "sequence": "supported",
    "set": "supported",
    "map": "supported",
    "nothing": "supported",
    "undefined": "supported",

    "int8": "reduced",
    "uint8": "reduced",
    "int16": "reduced",
    "uint16": "reduced",
    "int32": "reduced",
    "uint32": "reduced",
    "int64": "reduced",
    "uint64": "reduced",
    "bigint": "supported",
    "single_precision": "reduced",
    "double_precision": "reduced",
    "double_extended": "reduced",
    "fixed_precision": "unsupported",
    "array": "reduced",
    "list": "reduced",
    "tuple": "reduced"
  },
  "restrictions": {
    "map_key": [
      "text"
    ],
    "set": [
      "integer",
      "real",
      "text",
      "boolean",
      "sequence",
      "set",
      "map",
      "function_calls",
      "identifiers"
    ]
  }
}
