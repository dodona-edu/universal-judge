{
  "general": {
    "dependencies": [
      "values.py",
      "evaluation_utils.py"
    ],
    "selector": false
  },
  "extensions": {
    "file": "py"
  },
  "naming_conventions": {
    "class": "pascal_case",
    "global_identifier": "macro_case"
  },
  "constructs": {
    "objects": true,
    "exceptions": true,
    "function_calls": true,
    "assignments": true,
    "heterogeneous_collections": true,
    "heterogeneous_arguments": true,
    "named_arguments": true,
    "evaluation": true,
    "default_parameters": true,
    "global_variables": true,
    "method_references": true,
    "lambdas": true,
    "untyped_lambda_parameters": true,
    "typed_lambda_parameters": true,
    "assignments_lambda": true
  },
  "datatypes": {

    "integer": "supported",
    "rational": "supported",
    "char": "reduced",
    "text": "supported",
    "boolean": "supported",
    "sequence": "supported",
    "set": "supported",
    "map": "supported",
    "nothing": "supported",
    "undefined": "reduced",

    "int8": "reduced",
    "uint8": "reduced",
    "int16": "reduced",
    "uint16": "reduced",
    "int32": "reduced",
    "uint32": "reduced",
    "int64": "reduced",
    "uint64": "reduced",
    "bigint": "supported",
    "single_precision": "reduced",
    "double_precision": "reduced",
    "double_extended": "reduced",
    "fixed_precision": "supported",
    "array": "reduced",
    "list": "supported",
    "tuple": "supported"
  },
  "restrictions": {
    "map_key": [
      "integer",
      "rational",
      "text",
      "boolean",
      "tuple",
      "bigint",
      "fixed_precision",
      "function_calls",
      "identifiers"
    ],
    "set": [
      "integer",
      "rational",
      "text",
      "boolean",
      "tuple",
      "bigint",
      "fixed_precision",
      "function_calls",
      "identifiers"
    ]
  }
}
