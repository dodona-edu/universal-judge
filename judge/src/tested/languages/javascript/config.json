{
  "general": {
    "dependencies": [
      "values.js"
    ],
    "selector": false
  },
  "extensions": {
    "file": "js",
    "templates": ["js", "mako"]
  },
  "naming_conventions": {
    "namespace": "camel_case",
    "function": "camel_case"
  },
  "constructs": {
    "objects": true,
    "exceptions": true,
    "function_calls": true,
    "assignments": true,
    "heterogeneous_collections": true,
    "heterogeneous_arguments": true,
    "evaluation": true
  },
  "datatypes": {
    "integer": "supported",
    "rational": "supported",
    "char": "supported",
    "text": "supported",
    "boolean": "supported",
    "sequence": "supported",
    "set": "supported",
    "map": "supported",
    "nothing": "supported",

    "int8": "reduced",
    "uint8": "reduced",
    "int16": "reduced",
    "uint16": "reduced",
    "int32": "reduced",
    "uint32": "reduced",
    "int64": "reduced",
    "uint64": "reduced",
    "bigint": "reduced",
    "single_precision": "reduced",
    "double_precision": "reduced",
    "double_extended": "reduced",
    "fixed_precision": "reduced",
    "array": "reduced",
    "list": "reduced",
    "tuple": "unsupported"
  }
}
